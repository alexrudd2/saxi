{
    "$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
    "formatter": {
      "enabled": false
    },
    "linter": {
      "enabled": true,
      "rules": {
        "style": {
            "useNumberNamespace": "off"
        }
      }
    },
    "overrides": [
      {
        "include": ["__tests__/**"],
        "linter": {
          "rules": {
            "suspicious": {
              "noExplicitAny": "off"
            }
          }
        }
      }
    ],
    "organizeImports": {
      "enabled": true
    }
  }